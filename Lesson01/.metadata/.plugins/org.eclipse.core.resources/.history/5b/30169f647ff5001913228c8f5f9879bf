package lesson2;

public class lesson2 {

	public static void main(String[] args) {
		// TODO 自動生成されたメソッド・スタブ
		int a = 1;
		if(a==0){
			System.out.println("aは0に等しい");
		}else if(a==1){
			System.out.println("aは1に等しい");
		}
		
		switch(a){
		case 0:
			System.out.println("aは0に等しい");
			break;
		case 1:
			System.out.println("aは1に等しい");
			break;
		}

	}

}
